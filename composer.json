{
    "name": "unicodeveloper/cool-ascii-faces",
    "description": "Show cool ascii faces in your commandline",
    "license": "MIT",
    "keywords": [
        "busayo",
        "ascii",
        "cool",
        "faces",
        "ascii-faces",
        "cool-ascii-faces",
    ],
    "authors": [
        {
            "name": "busayo",
            "email": "prosperotemuyiwa@gmail.com"
        }
    ],
    "require": {
        "php" : ">=5.3.0"
    },
    "require-dev": {
        "phpunit/phpunit" : "4.*",
        "scrutinizer/ocular": "~1.1"
    },
    "autoload": {
        "psr-4": {
            "Busayo\\" : "src/"
        },
        "classmap": ["src/"]
    },
    "autoload-dev": {
        "psr-4": {
            "Busayo\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
